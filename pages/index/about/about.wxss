/* pages/index/contactUs/contactUs.wxss */
.body{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.mianImg{
  width: 100%;
  height: 380rpx;
}
.container{
  padding: 30rpx; 
  box-sizing: border-box;

}
.title{
  font:bold 50rpx "微软雅黑";
  color:#0066FF;
}
.subtitle{
  font-size: 25rpx;
  color: #666;
  margin-bottom: 25rpx;
}
.subtitle text{
  padding: 25rpx 0;
  display: inline-block;
  border-bottom:1px solid #FF7852;
}
.content{
  font-size: 30rpx;
  color: #333;
  margin-bottom: 20rpx;
}
.articleImg{
  width: 100%;
}




.contact-list{
  display:flex;
  align-items: center;
  border-bottom:1px solid #eee;
  padding: 30rpx 0;
}
.name{
  font-size:30rpx;
  color:#000;
  flex:1;
}
.detail{
  flex:3;
  font-size:30rpx;
  color:#999;
  text-align:right;
}